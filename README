# usage:
cd code
git clone git@github.com:murkey/dot_files.git
cd dot_files
git submodule init
git submodule update
cd .vim/bundle/Command-T/ruby/command-t
ruby extconf.rb
make
cd $HOME
ln -s $HOME/code/dot_files/.ackrc .
ln -s $HOME/code/dot_files/.bash_profile .
ln -s $HOME/code/dot_files/.bashrc .
ln -s $HOME/code/dot_files/.gemrc .
ln -s $HOME/code/dot_files/.gitignore .
ln -s $HOME/code/dot_files/.gitmodules .
ln -s $HOME/code/dot_files/.gvimrc .
ln -s $HOME/code/dot_files/.profile .
ln -s $HOME/code/dot_files/.vim .
ln -s $HOME/code/dot_files/.vimrc .
ln -s $HOME/code/dot_files/.zsh .
ln -s $HOME/code/dot_files/.zshrc .

